-- Create company tablet
create table company (
	cid serial primary key,
	name varchar(200),
    income float,
    allIncome float,
    parentId int
);

USE companies;
CREATE TABLE company (cid int not null auto_increment, primary key (cid), name VARCHAR(250),
    price float, parentid int);


--Insert temp values to table
INSERT INTO company (name, income, allincome, parentid)
VALUES (1, 'Eleks', 450.1, 700.1, 7)

--Delete row from table by id
DELETE FROM company
WHERE cid = 1;

